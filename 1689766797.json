{
  "title": "MacLee's MacOS Mouse 4/5 Changer (4/5: move tabs)",
  "rules": [
    {
      "description": "Mouse 4 Backward Tab Move",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.apple\\.Safari$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "open_bracket",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "Mouse 5 Forward Tab Move",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.apple\\.Safari$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "type": "basic",
          "from": {
            "pointing_button": "button5"
          },
          "to": [
            {
              "repeat": false,
              "key_code": "close_bracket",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    }
  ]
}